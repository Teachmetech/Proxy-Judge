{
    "name": "proxy-judge-monorepo",
    "version": "1.0.0",
    "description": "Proxy Judge - Frontend and Backend Monorepo",
    "private": true,
    "scripts": {
        "dev": "concurrently \"pnpm --filter frontend dev\" \"pnpm --filter backend dev\"",
        "dev:frontend": "pnpm --filter frontend dev",
        "dev:backend": "pnpm --filter backend dev",
        "build": "pnpm --filter frontend build",
        "start:frontend": "pnpm --filter frontend preview",
        "start:backend": "pnpm --filter backend start",
        "install:all": "pnpm install",
        "clean": "rm -rf frontend/node_modules backend/node_modules frontend/dist"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    },
    "keywords": [
        "proxy",
        "judge",
        "anonymity",
        "api",
        "monorepo"
    ],
    "author": "",
    "license": "ISC",
    "engines": {
        "node": ">=18.0.0"
    }
}